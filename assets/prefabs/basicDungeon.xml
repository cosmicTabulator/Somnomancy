  <!-- Adds #count rectangular rooms to the pool, each with #doors doors-->
  <rectangular width="" height="" count="" doors="" />
  <!-- Defines the prefab room pool-->
  <features>
    <prefab filepath="" definition="" symmetry="" weight=""/>
    <prefab filepath="" definition="" symmetry="" weight=""/>
    <prefab filepath="" definition="" symmetry="" weight=""/>
    <prefab filepath="" definition="" symmetry="" weight=""/>
    <prefab filepath="" definition="" symmetry="" weight=""/>
