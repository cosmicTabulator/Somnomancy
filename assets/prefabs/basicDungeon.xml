  <!-- Placements after generation is complete (for stairs, etc)-->
  <end>
    <prefab filepath="" definition="" symmetry="" weight=""/>
    <prefab filepath="" definition="" symmetry="" weight=""/>
  </end>
  <!-- Added to prefab room pool, attempt descarded if failed to place -->
  <require>
